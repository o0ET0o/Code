@inherits ViewBase<dynamic>
@using z.ERP.Web.Areas.Share.Render
@using z.MVC5.Views
@{
    Layout = ThisLayout;
}
@{
    WebFiles = new string[] {
                };
}

@section head{
    @InitFiles(WebFiles)
    @RenderSection("head", false)
}


<div id="List_Main">
    <collapse>
        <row class="topbutton">
            @RenderSection("btn", false)
        </row>
        <row>
            <card>
                @RenderSection("content", true)
            </card>
        </row>

    </collapse>
</div>

@section foot{
    @RenderSection("foot", false)
}