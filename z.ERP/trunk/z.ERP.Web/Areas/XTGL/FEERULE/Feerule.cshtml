@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Web.Areas.Layout.Define
@using z.ERP.Entities.Enum
@inherits DefineViewBase

@section element{
    <row>
        <i-col  span="3" class="RowTitle">
            收费规则代码
        </i-col>
        <i-col span="6">
            @{ 
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.ID",
                    Id = "tb_code",
                    Readonly = "true"
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            收费规则名称
        </i-col>
        <i-col span="6">
            @{ 
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.NAME",
                    Id = "tb_name",
                    Enable = false,
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            缴费周期
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.PAY_CYCLE",
                    Id = "tb_paycycle",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            出单日期
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.FEE_DAY",
                    Id = "tb_advancecycle",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            提前周期
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.ADVANCE_CYCLE",
                    Id = "tb_advancecycle",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            缴费截至日
        </i-col>
        <i-col span="6">
            @{ 
                RenderControl(new TextBoxRender() {
                    vModel = "dataParam.UP_DATE",
                    Id = "tb_upd",
                    Enable = false,
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="3" class="RowTitle">
            缴费截至周期
        </i-col>
        <i-col span="6">
            @{ 
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.PAY_UP_CYCLE",
                    Id = "tb_payupcycle",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="3" class="RowTitle">
            作废标记
        </i-col> 
        <i-col span="6">
            @{ 
                RenderControl(new EnumDropDownListRender<停用标记>()
                {
                    vModel = "dataParam.VOID_FLAG",
                    Id = "tb_voidflag",
                    Enable = false
                });
            }
        </i-col>
    </row>
}