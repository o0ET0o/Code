@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Web.Areas.Layout.Define
@using z.ERP.Entities.Enum
@inherits DefineViewBase

@section element{
    <row>
        <i-col  span="3" class="RowTitle">
            滞纳规则代码
        </i-col>
        <i-col span="6">
            @{ 
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.ID",
                    Id = "tb_id",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            滞纳规则名称
        </i-col>
        <i-col span="6">
            @{ 
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.NAME",
                    Id = "tb_name",
                    Enable = false,
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            宽限天数
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.DAYS",
                    Id = "tb_days",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="3" class="RowTitle">
            宽限金额
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.AMOOUNTS",
                    Id = "tb_amounts",
                    Enable = false
                });
            }
        </i-col>
    </row> 
}