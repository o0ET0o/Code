@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Define
@inherits DefineViewBase

@section element{
    <row>
        <i-col span="2" class="RowTitle">分店</i-col>
        <i-col span="6">
            @{
                RenderControl(new ServiceDropDownListRender()
                {
                    vModel = "dataParam.BRANCHID",
                    ServiceMothod = m => m.branch(),
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">楼层</i-col>
        <i-col span="6">
            @{
                RenderControl(new ServiceDropDownListRender()
                {
                    vModel = "dataParam.FLOORID",
                    ServiceMothod = m => m.floor(),
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">单元编号</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.SHOPID",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">单元代码</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.CODE",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">单元名称</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.NAME",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">单元类型</i-col>
        <i-col span="6">
            @{
                RenderControl(new EnumDropDownListRender<单元类型>()
                {
                    vModel = "dataParam.TYPE",
                    Enable = false,
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">管理部门</i-col>
        <i-col span="6">
            @{
                RenderControl(new UndefineRender()
                {
                    vModel = "dataParam.ORGID",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">业态</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.CATEGORYID",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">建筑面积</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.AREA_BUILD",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">使用面积</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.AREA_USABLE",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">租赁面积</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.AREA_RENTABLE",
                    Enable = false
                });
            }
        </i-col>
    </row>

<row>
    <i-col span="2" class="RowTitle">单元状态</i-col>
    <i-col span="6">
        @{
            RenderControl(new EnumCheckBoxListRender<单元状态>()
            {
                vModel = "dataParam.STATUS",
                MultiSelect = false,
                Enable = false
            });
        }
    </i-col>

    <i-col span="2" class="RowTitle">租用状态</i-col>
    <i-col span="6">
        @{
            RenderControl(new EnumCheckBoxListRender<租用状态>()
            {
                vModel = "dataParam.RENT_STATUS",
                MultiSelect = false,
                Enable = false
            });
        }
    </i-col>
</row>





}