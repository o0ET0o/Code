@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Web.Areas.Layout.Define
@using z.ERP.Entities.Enum
@inherits DefineViewBase

@section element{
    <row>
        <i-col span="2" class="RowTitle">
            POS终端号
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.STATIONBH",
                    Id = "tb_bh",
                    Enable =false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            类型
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new EnumDropDownListRender<POS类型>
                {
                    vModel = "dataParam.TYPE",
                    Id = "tb_type",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            IP
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.IP",
                    Id = "tb_ip",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            收款方式
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new EnumCheckBoxListRender<POS类型>
                {
                    vModel = "dataParam.SKFS",
                    Id = "tb_ip",
                    Enable = false,
                    MultiSelect =true
                });
            }
        </i-col>
    </row>








    
}
