@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Define
@inherits DefineViewBase

@section element{
    <row>
        <i-col span="2" class="RowTitle">设备编号</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.FILEID",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">设备代码</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.FILECODE",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">设备名称</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.FILENAME",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">单价</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.PRICE",
                    Enable = false,
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">最后读数</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.VALUE_LAST",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">最后度数日期</i-col>
        <i-col span="6">
            @{
                RenderControl(new DateBoxRender()
                {
                    vModel = "dataParam.DATE_LAST",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">倍率</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.MULTIPLE",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">描述</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.DESCRIPTION",
                    Enable = false
                });
            }
        </i-col>
    </row>

    <row>
        <i-col span="2" class="RowTitle">区域</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.AREAID",
                    Enable = false
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">店铺</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "dataParam.SHOPID",
                    Enable = false
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">出厂日期</i-col>
        <i-col span="6">
            @{
                RenderControl(new DateBoxRender()
                {
                    vModel = "dataParam.FACTORY_DATE",
                    Enable = false
                });
            }
        </i-col>

    </row>



















}