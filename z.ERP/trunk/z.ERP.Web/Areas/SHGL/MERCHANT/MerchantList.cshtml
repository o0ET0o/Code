@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Search;
@inherits SearchViewBase
@section conditionParam{
<row>
    <i-col span="2" class="RowTitle">商户代码</i-col>
    <i-col span="6">
        @{
            RenderControl(new TextBoxRender()
            {
                vModel = "searchParam.MERCHANTID" ,
            });
        }
    </i-col>

    <i-col span="2" class="RowTitle">商户名称</i-col>
    <i-col span="6">
        @{
            RenderControl(new TextBoxRender()
            {
                vModel = "searchParam.MERCHANTNAME",
            });
        }
    </i-col>

    <i-col span="2" class="RowTitle">税号</i-col>
    <i-col span="6">
        @{
            RenderControl(new TextBoxRender()
            {
                vModel = "searchParam.SH"
            });
        }
    </i-col>
</row>

 <row>
     <i-col span="2" class="RowTitle">银行账号</i-col>
     <i-col span="6">
         @{
             RenderControl(new TextBoxRender()
             {
                 vModel = "searchParam.BANK",
             });
         }
     </i-col>

     <i-col span="2" class="RowTitle">状态</i-col>
     <i-col span="6">
         @{
             RenderControl(new EnumCheckBoxListRender<普通单据状态>()
             {
                 vModel = "searchParam.STATUS",
                 MultiSelect = true
             });
         }
     </i-col>

 </row>
}
